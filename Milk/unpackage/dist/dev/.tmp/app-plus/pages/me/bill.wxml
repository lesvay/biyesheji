<view class="billainer"><view class="qiun-charts"><view style="text-align:center;width:100%;">{{newuser}}</view><canvas class="charts" canvas-id="canvasLineA" id="canvasLineA" disable-scroll="true" data-event-opts="{{[['touchstart',[['touchLineA',['$event']]]],['touchmove',[['moveLineA',['$event']]]],['touchend',[['touchEndLineA',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"></canvas></view><view class="billBg"><block wx:for="{{billList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['godetail',['$0'],[[['billList','',index,'goods_rfid']]]]]]]}}" class="billOver" bindtap="__e"><view class="idxMainBox"><view class="billOverLt"><view class="billName">{{item.goods_name}}</view><view class="billBuytime">{{"购买日期："+item.goods_buytime}}</view><view class="billdeadtime">{{"截止日期："+item.goods_deadtime}}</view><view class="billmachnum">{{"机器编号："+item.goods_machnum}}</view></view><view class="billOverRt"><view class="billPrice">{{"￥"+item.goods_price+"元"}}</view><view class="billMore">查看更多>></view></view></view></view></block></view></view>