<view class="countainer"><view class="qiun-charts"><view style="text-align:center;width:100%;">{{newuser}}</view><canvas class="charts" canvas-id="canvasLineA" id="canvasLineA" disable-scroll="true" data-event-opts="{{[['touchstart',[['touchLineA',['$event']]]],['touchmove',[['moveLineA',['$event']]]],['touchend',[['touchEndLineA',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"></canvas></view><view class="countBg"><block wx:for="{{countList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['godetail',['$0'],[[['countList','',index,'goods_rfid']]]]]]]}}" class="countOver" bindtap="__e"><view class="idxMainBox"><view class="countOverLt"><view class="countName">{{item.goods_name}}</view><view class="countBuytime">{{"购买日期："+item.goods_buytime}}</view><view class="countmember">{{"购买会员："+item.goods_member}}</view><view class="countmachnum">{{"机器编号："+item.goods_machnum}}</view></view><view class="countOverRt"><view class="countPrice">{{"￥"+item.goods_price+"元"}}</view><view class="countMore">查看更多>></view></view></view></view></block></view></view>